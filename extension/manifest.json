{
    "name": "Bait Blocker",
    "version": "1.0.0",
    "description": "Bait Blocker",
    "manifest_version": 3,
    "author": "us",
    "action": {
        "default_popup": "index.html",
        "default_title": "Bait Blocker"
    },
    "permissions": [
        "storage",
        "tabs",
        "http://100.64.161.177:5000/"
    ],
    "host_permissions": ["http://100.64.161.177:5000/"],

    "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self'"
    },
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "baitblasocker2.png",
        "48": "baitblasocker2.png",
        "128": "baitblasocker2.png"
      }
    },
    "background": {
      "background.service_worker": ["script.js"]
      
    }
}